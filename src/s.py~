def get_s():
	
	shares = []
	
	shares.append('GOOG')
	shares.append('XRX')
	shares.append('MSFT')
	shares.append('CSCO')
	shares.append('AIG')
	shares.append('NFLX')
	shares.append('YHOO')
	shares.append('EBAY')
	shares.append('BAC')
	shares.append('HPQ')
	
	return shares
	
